@inherits AnchoredComponentBase

<Space id="@Id" class="@Class" style="@Style" as="@As" centerContent="@CenterContent" zIndex="@ZIndex"
    scrollable="@Scrollable" trackSize="@TrackSize" type="@SpaceType.Anchored" anchor="@AnchorType.Top"
    position='@PositionalProps.Create().Left("0").Right("0").Top("0").Height(Size).BottomResizable()' order="@Order"
    handlePlacement='@HandlePlacement'>
    @ChildContent
</Space>
